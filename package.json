{
	"name": "@ruan-cat/drill-doc",
	"description": "阮喵喵维护的钻头文档。",
	"private": true,
	"version": "0.0.1",
	"author": {
		"name": "ruan-cat"
	},
	"license": "MIT",
	"type": "module",
	"packageManager": "pnpm@9.5.0",
	"engines": {
		"node": ">=20.15.1",
		"vscode": "^1.22.0",
		"pnpm": ">=9.5.0"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-git"
		}
	},
	"scripts": {
		"git:fetch": "git fetch -p",
		"git:dev-2-main": "git rebase dev main",
		"git:main-2-dev": "git rebase main dev",
		"docs:build": "vuepress-vite build docs",
		"docs:dev": "vuepress-vite dev docs --clean-cache --open",
		"docs:update-package": "pnpm dlx vp-update",
		"up-ruan-cat": "pnpm -F=* up @ruan-cat/*@latest",
		"postinstall": "npx nuxi prepare",
		"rm:node_modules": "rimraf node_modules pnpm-lock.yaml .nuxt .vercel",
		"deploy-vercel": "node --import=tsx ./scripts/vercel-deploy-tool.ts",
		"build:drill-docx2md": "node --import=tsx ./scripts/docx2md/index.ts"
	},
	"devDependencies": {
		"@ruan-cat/commitlint-config": "^0.1.3",
		"@ruan-cat/domains": "^0.1.3",
		"@ruan-cat/utils": "^1.4.2",
		"@ruan-cat/vercel-deploy-tool": "^0.5.6",
		"@ruan-cat/vuepress-preset-config": "^0.1.18",
		"@types/degit": "^2.8.6",
		"@types/lodash-es": "^4.17.12",
		"@vuepress/bundler-vite": "2.0.0-rc.19",
		"commitizen": "^4.3.1",
		"consola": "^3.4.0",
		"cz-git": "^1.11.0",
		"degit": "^2.8.4",
		"gradient-string": "^3.0.0",
		"lodash-es": "^4.17.21",
		"mermaid": "^10.9.3",
		"nuxt": "^3.15.4",
		"tsx": "^4.19.2",
		"vue": "^3.5.13",
		"vuepress": "2.0.0-rc.19"
	}
}